        function showRandomNumbers(){
            document.getElementById("myNumber").innerHTML = rando(1, 37); 
            document.getElementById("Number").innerHTML = rando(1, 37);
            document.getElementById("umber").innerHTML = rando(1, 37);
            document.getElementById("mNumber").innerHTML = rando(1, 37);
            document.getElementById("liel").innerHTML = rando(1, 37);
            document.getElementById("mishel").innerHTML = rando(1, 37);
            document.getElementById("me").innerHTML = rando(1, 7);
        }
    </script>
